<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
    res.redirect('/')
} %>

<div id="account-management">
    <h2>Welcome <%= locals.accountData.account_firstname%></h2>
    <p>You're logged in</p>
    <a href="./update/<%= locals.accountData.account_id %>">Edit Account Information</a>
</div>

<% if (accountData.account_type !== 'Client') { %>
<div>
    <h3> Inventory Management</h3>
    <a href="/inv/">Management Inventory</a>
</div>
<% } %>